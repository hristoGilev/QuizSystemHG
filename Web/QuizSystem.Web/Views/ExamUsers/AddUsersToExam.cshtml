@model QuizSystem.Web.ViewModels.ExamUses.ExamUsersview
@{
    this.ViewData["Title"] = "Add user to exam";
}
<h3>@this.ViewData["Title"]</h3>
@{ var parm = new Dictionary<string, string>
{
                { "idexam", Model.ExamId.ToString() },
              
            };
   
}

            
<form method="post"  asp-all-route-data="parm">

    @foreach (var item in Model.examUsers)
    {
        <div class="form-check">

         <input type="checkbox" name="username" value="@item.UserName"/><label><span style="margin-left:3px">@item.UserName</span></label>
        </div>

    }
    <input type="submit" class="btn btn-primary" />
</form>
